{
  "name": "dotkernel/dot-rbac",
  "type": "library",
  "description": "DotKernel RBAC authorization component",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "container-interop/container-interop": "^1.1",
    "zendframework/zend-servicemanager": "^3.1",

    "dotkernel/dot-authorization": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",

    "zendframework/zend-stdlib": "^3.1",
    "dotkernel/dot-authentication": "~0.1"
  },
  "suggest": {
    "dotkernel/dot-authentication": "Needed if you want to use the AuthenticationIdentityProvider"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Rbac\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Rbac\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  }
}
